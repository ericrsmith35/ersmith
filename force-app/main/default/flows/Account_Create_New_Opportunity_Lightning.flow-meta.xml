    </formulas>
    <formulas>
        <name>fLeft3</name>
        <dataType>String</dataType>
        <expression>UPPER(LEFT({!vStartMode},3))</expression>
    </formulas>
    <formulas>
        <name>fOppMode</name>
        <dataType>String</dataType>
        <expression>RIGHT({!vStartMode},LEN({!vStartMode})-3)</expression>
    </formulas>
    <formulas>
        <name>fOppName</name>
        <dataType>String</dataType>
        <expression>{!vAccountName} &amp; &quot; - &quot;</expression>
    </formulas>
    <formulas>
        <description>120 character max for Opp Name</description>
        <name>fOppName120</name>
        <dataType>String</dataType>
        <expression>IF(LEN({!ttOppName})&lt;121,{!ttOppName},
LEFT({!ttOppName},59) &amp; &quot;...&quot; &amp; RIGHT({!ttOppName},58))</expression>
    </formulas>
    <formulas>
        <name>fOppType</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!vPlatID}),&quot;New Customer&quot;, &quot;New Agreement&quot;)</expression>
    </formulas>
    <formulas>
        <name>fUpperMode</name>
        <dataType>String</dataType>
        <expression>UPPER({!vStartMode})</expression>
    </formulas>

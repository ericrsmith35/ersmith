        <name>fClickHere</name>
        <dataType>String</dataType>
        <expression>&#39;&lt;A HREF=&quot;&#39; &amp; {!$Label.GWI_Salesforce_Domain} &amp; {!vOppId} &amp; &#39;/e?retURL=%2F006%2Fo&amp;ent=Opportunity &quot; TARGET=&quot;_blank&quot;&gt;&#39; &amp; 
